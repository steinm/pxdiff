#: src/main.c:537
msgid "Not supported field type."
msgstr "Not unterstützter Feldtyp."

#: src/main.c:574
#, c-format
msgid "Version: %s %s http://sourceforge.net/projects/pxlib"
msgstr "Version: %s %s http://sourceforge.net/projects/pxlib"

#: src/main.c:576
