#: src/main.c:1485
#, c-format
msgid "%s compares two paradox databases."
msgstr "%s vergleicht zwei Paradox Datenbanken."

#: src/main.c:1487
#, c-format
msgid "Usage: %s [OPTIONS] DATABASE1 DATABASE2 [PRIINDEX1 PRIINDEX2]"
msgstr "Aufruf: %s [OPTION] DATABASE1 DATABASE2 [PRIINDEX1 PRIINDEX2]"

#: src/main.c:1489
#, c-format
msgid "Options:"
msgstr "Optionen:"

#: src/main.c:1491
#, c-format
msgid "  -h, --help          this usage information."
msgstr "  -h, --help          diese Hilfe."

#: src/main.c:1493
#, c-format
msgid "  --version           show version information."
msgstr "  --version           zeige Programmversion."

#: src/main.c:1495
#, c-format
msgid "  -v, --verbose       be more verbose."
msgstr "  -v, --verbose       mehr Informationen anzeigen"

#: src/main.c:1497
#, c-format
msgid "  -d, --data          compare data."
msgstr "  -d, --data          vergleiche Datensätze"

#: src/main.c:1499
#, c-format
msgid "  -t, --schema        compare schema (default)."
msgstr "  -t, --schema        vergleiche Tabellenstuktur (Voreinstellung)"

#: src/main.c:1501
#, c-format
msgid "  -s, --sort          sort data before calculating difference."
msgstr "  -s, --sort          sortiere Datensätze vor der Differenzenbildung."

#: src/main.c:1503
#, c-format
msgid "  --mode=MODE         set compare mode (schema (default), data)."
msgstr ""
"  --mode=MODE         setze Vergleichs-Modus (schema (voreingestellt), data)"

#: src/main.c:1505
#, c-format
msgid "  --output-mode=MODE  set output mode (plain (default), sql, stat)."
msgstr ""
"  --output-mode=MODE  setze Ausgabe-Modus (plain (voreingestellt), sql, stat)"

#: src/main.c:1507
#, c-format
msgid "  -o, --output-file=FILE output data into file instead of stdout."
msgstr "  -o, --output-file=DATEI schreibe Ausgabe in Datei statt nach stdout."

#: src/main.c:1509
#, c-format
msgid "  -k, --primary-key=FIELD use field as primary key."
msgstr "  -k, --primary-key=FELD Benutze Feld als Primärschlüssel."

#: src/main.c:1511
#, c-format
msgid "  -r, --recode=ENCODING sets the target encoding."
msgstr "  -r, --recode=ENCODING setzt Zielkodierung"

#: src/main.c:1513
#, c-format
msgid "  --fields=REGEX      extended regular expression to select fields."
msgstr ""
"  --fields=REGEX      erweiterter regulärer Ausdruck zur Auswahl von Feldern."

#: src/main.c:1515
#, c-format
msgid ""
"  --compare-common    compare only those fields common in both databases."
msgstr ""
"  --compare-common    vergleiche nur Felder, die in beiden Datenbanken sind."

#: src/main.c:1517
#, c-format
msgid ""
"  --disregard-codepage  different code pages will not prevent record\n"
"                      comparision."
msgstr ""
"  --disregard-codepage  unterschiedliche code pages verhindern nicht den\n"
"                      Vergleich der Datensätze. "

#: src/main.c:1519
#, c-format
msgid "  --show-record-diff  Show field differences within a record."
msgstr ""
"  --show-record-diff  Zeige Feldwertunterschiede innerhalb eines Datensatzes."

#: src/main.c:1523
#, c-format
msgid "  --use-gsf           use gsf library to read input file."
msgstr ""
"  --use-gsf           benutze gsf-Bibliothek zum Lesen der Eingabedatei."

