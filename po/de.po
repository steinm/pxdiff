#: src/main.c:451
#, c-format
msgid "%s compares two paradox databases."
msgstr "%s vergleich zwei Paradox Datenbanken."

#: src/main.c:453
#, c-format
msgid "Usage: %s [OPTIONS] FILE1 FILE2"
msgstr "Aufruf: %s [OPTION] DATEI1 DATEI2"

#: src/main.c:455
msgid "Options:"
msgstr "Optionen:"

#: src/main.c:457
msgid "  -h, --help          this usage information."
msgstr "  -h, --help          diese Hilfe."

#: src/main.c:459
msgid "  -v, --verbose       be more verbose."
msgstr "  -v, --verbose       mehr Informationen anzeigen"

#: src/main.c:461
msgid "  -d, --data          compare data."
msgstr "  -d, --data          vergleiche Datensätze"

#: src/main.c:463
msgid "  -t, --schema        compare schema."
msgstr "  -t, --schema        vergleiche Tabellenstuktur"

#: src/main.c:465
msgid "  --mode=MODE         set compare mode (schema, data)."
msgstr "  --mode=MODE         setze Vergleichs-Modus (schema, data)"

#: src/main.c:467
msgid "  -o, --output-file=FILE output data into file instead of stdout."
msgstr "  -o, --output-file=DATEI schreibe Ausgabe in Datei statt nach stdout."

#: src/main.c:469
msgid "  -n, --primary-key=FIELD use field as primary key."
msgstr "  -n, --primary-key=FELD Benutze Feld als Primärschlüssel."

#: src/main.c:471
msgid "  -r, --recode=ENCODING sets the target encoding."
msgstr "  -r, --recode=ENCODING setzt Zielkodierung"

#: src/main.c:473
msgid "  --fields=REGEX      extended regular expression to select fields."
msgstr ""
"  --fields=REGEX      erweiterter regulärer Ausdruck zur Auswahl von Feldern."

#: src/main.c:477
msgid "  --use-gsf           use gsf library to read input file."
msgstr ""
"  --use-gsf           benutze gsf-Bibliothek zum Lesen der Eingabedatei."

#: src/main.c:486
msgid "libpx uses librecode for recoding."
msgstr "libpx benutzt librecode zum reencodieren."

#: src/main.c:489
msgid "libpx uses iconv for recoding."
msgstr "libpx benutzt iconv zum reencodieren."

#: src/main.c:492
msgid "libpx has no support for recoding."
msgstr "libpx unterstützt nicht das reencodieren."

#: src/main.c:497
msgid "libpx has been compiled for big endian architecture."
msgstr "libpx unterstützt nicht das reencodieren."

#: src/main.c:499
msgid "libpx has been compiled for little endian architecture."
msgstr "libpx wurde für Architekturen mit little endian übersetzt."

#: src/main.c:501
#, c-format
msgid "libpx has gsf support: %s"
